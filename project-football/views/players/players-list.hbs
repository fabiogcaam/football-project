<div class="container mt-5">
    <h1>Lista de jugadores</h1>
    <hr>
    {{#each data}}
    <div class="row mt-5">
        <div class="col-9">
            <h5 class="card-title">{{name}}</h5>
        </div>
        <div class="col-3">
            <a href="/players/{{id}}" class="btn btn-dark">Ver detalles</a>
        </div>
    </div>
    {{/each}}
</div>